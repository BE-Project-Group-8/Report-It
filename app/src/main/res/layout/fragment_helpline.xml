<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background_for_app"
    android:padding="0dp"
    android:paddingLeft="7dp"
    android:paddingTop="0dp"
    android:paddingRight="0dp"
    android:paddingBottom="0dp"
    tools:context=".fragment_helpline">


    <TextView
        android:id="@+id/title_helpline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="HELPLINE NUMBERS"
        android:textColor="@color/heading_title"
        android:textSize="36dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.023" />

    <TextView
        android:id="@+id/txtNationalHelpline"
        android:layout_width="192dp"
        android:layout_height="78dp"
        android:gravity="center_horizontal"
        android:text="NATIONAL HELPLINE"
        android:textColor="#FFFFFF"
        android:textSize="28dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.073"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/title_helpline"
        app:layout_constraintVertical_bias="0.067" />

    <Button
        android:id="@+id/btnNationalHelpCall"
        android:layout_width="125dp"
        android:layout_height="55dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="24dp"
        android:layout_marginRight="24dp"
        android:backgroundTint="@color/heading_title"
        android:gravity="center_horizontal"
        android:text="CALL"
        android:textColor="#000000"
        android:textSize="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/txtNationalHelpline"
        app:layout_constraintTop_toBottomOf="@+id/title_helpline"
        app:layout_constraintVertical_bias="0.026" />

    <TextView
        android:id="@+id/txtPolice"
        android:layout_width="162dp"
        android:layout_height="39dp"
        android:gravity="center_horizontal"
        android:text="POLICE"
        android:textColor="#FFFFFF"
        android:textSize="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.064"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtNationalHelpline"
        app:layout_constraintVertical_bias="0.064" />

    <Button
        android:id="@+id/btnPoliceCall"
        android:layout_width="125dp"
        android:layout_height="55dp"
        android:layout_marginTop="32dp"
        android:backgroundTint="@color/heading_title"
        android:gravity="center_horizontal"
        android:text="CALL"
        android:textColor="#000000"
        android:textSize="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.768"
        app:layout_constraintStart_toEndOf="@+id/txtPolice"
        app:layout_constraintTop_toBottomOf="@+id/btnNationalHelpCall"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/txtAmbulance"
        android:layout_width="201dp"
        android:layout_height="45dp"
        android:gravity="center_horizontal"
        android:text="AMBULANCE"
        android:textColor="#FFFFFF"
        android:textSize="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.076"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtPolice"
        app:layout_constraintVertical_bias="0.09" />


    <Button
        android:id="@+id/btnAmbulanceCall"
        android:layout_width="125dp"
        android:layout_height="55dp"
        android:layout_marginStart="44dp"
        android:layout_marginLeft="44dp"
        android:layout_marginTop="28dp"
        android:backgroundTint="@color/heading_title"
        android:gravity="center_horizontal"
        android:text="CALL"
        android:textColor="#000000"
        android:textSize="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/txtAmbulance"
        app:layout_constraintTop_toBottomOf="@+id/btnPoliceCall"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/txtFire"
        android:layout_width="154dp"
        android:layout_height="44dp"
        android:gravity="center_horizontal"
        android:text="FIRE"
        android:textColor="#FFFFFF"
        android:textSize="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.062"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtAmbulance"
        app:layout_constraintVertical_bias="0.137" />

    <Button
        android:id="@+id/btnFireCall"
        android:layout_width="125dp"
        android:layout_height="55dp"
        android:layout_marginStart="92dp"
        android:layout_marginLeft="92dp"
        android:layout_marginBottom="296dp"
        android:backgroundTint="@color/heading_title"
        android:gravity="center_horizontal"
        android:text="CALL"
        android:textColor="#000000"
        android:textSize="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/txtFire"
        app:layout_constraintTop_toBottomOf="@+id/btnAmbulanceCall"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/txtWomensHelpline"
        android:layout_width="197dp"
        android:layout_height="72dp"
        android:gravity="center_horizontal"
        android:text="WOMEN'S HELPLINE"
        android:textColor="#FFFFFF"
        android:textSize="28dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.074"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtFire"
        app:layout_constraintVertical_bias="0.165" />

    <Button
        android:id="@+id/btnWomensCall"
        android:layout_width="127dp"
        android:layout_height="55dp"
        android:layout_marginStart="48dp"
        android:layout_marginLeft="48dp"
        android:layout_marginTop="44dp"
        android:backgroundTint="@color/heading_title"
        android:gravity="center_horizontal"
        android:text="CALL"
        android:textColor="#000000"
        android:textSize="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/txtWomensHelpline"
        app:layout_constraintTop_toBottomOf="@+id/btnFireCall"
        app:layout_constraintVertical_bias="0.009" />

    <TextView
        android:id="@+id/txtSeniorCtzCall"
        android:layout_width="226dp"
        android:layout_height="72dp"
        android:gravity="center_horizontal"
        android:text="SENIOR CITIZEN HELPLINE"
        android:textColor="#FFFFFF"
        android:textSize="28dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.086"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtWomensHelpline"
        app:layout_constraintVertical_bias="0.234" />

    <Button
        android:id="@+id/btnSeniorCtzCall"
        android:layout_width="125dp"
        android:layout_height="55dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="40dp"
        android:backgroundTint="@color/heading_title"
        android:gravity="center_horizontal"
        android:text="CALL"
        android:textColor="#000000"
        android:textSize="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.107"
        app:layout_constraintStart_toEndOf="@+id/txtSeniorCtzCall"
        app:layout_constraintTop_toBottomOf="@+id/btnWomensCall"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/txtDisaster"
        android:layout_width="184dp"
        android:layout_height="50dp"
        android:layout_marginTop="24dp"
        android:gravity="center_horizontal"
        android:text="N.D.R.F"
        android:textColor="#FFFFFF"
        android:textSize="32dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.07"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtSeniorCtzCall" />

    <Button
        android:id="@+id/btnDisasterCall"
        android:layout_width="126dp"
        android:layout_height="54dp"
        android:layout_marginStart="60dp"
        android:layout_marginLeft="60dp"
        android:layout_marginTop="40dp"
        android:backgroundTint="@color/heading_title"
        android:gravity="center_horizontal"
        android:text="CALL"
        android:textColor="#000000"
        android:textSize="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.115"
        app:layout_constraintStart_toEndOf="@+id/txtDisaster"
        app:layout_constraintTop_toBottomOf="@+id/btnSeniorCtzCall"
        app:layout_constraintVertical_bias="0.083" />

</androidx.constraintlayout.widget.ConstraintLayout>